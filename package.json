{
  "name": "slack-kaldb-app",
  "version": "0.0.1",
  "description": "Grafana KalDB App",
  "scripts": {
    "build": "rm -rf dist && npx grafana-toolkit plugin:build",
    "build-go": "mage buildAll && mv dist/gpx* dist/datasource",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch",
    "zip": "zip slack-kaldb-app.zip dist/ -rj"
  },
  "author": "Slack",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "latest",
    "@grafana/ui": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/runtime": "latest",
    "@emotion/eslint-plugin": "11.2.0",
    "@types/react-virtualized-auto-sizer": "1.0.0"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "react-virtualized-auto-sizer": "1.0.2"
  },
  "resolutions": {
    "rxjs": "6.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}

