{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Astra",
  "id": "slack-astra-app",
  "executable": "gpx_slack_astra_app_datasource_backend",
  "info": {
    "description": "Grafana Astra App",
    "author": {
      "name": "Slack"
    },
    "keywords": ["Astra"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [{ "name": "Github", "url": "https://github.com/slackhq/astra" }],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },

  "includes": [
    {
      "type": "page",
      "name": "Explore",
      "path": "/a/slack-astra-app",
      "icon": "compass",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "datasource",
      "name": "Datasource",
      "icon": "database",
      "role": "Editor"
    }
  ],

  "dependencies": {
    "grafanaDependency": ">=10.0.0",
    "grafanaVersion": "10.0.0",
    "plugins": []
  }
}
